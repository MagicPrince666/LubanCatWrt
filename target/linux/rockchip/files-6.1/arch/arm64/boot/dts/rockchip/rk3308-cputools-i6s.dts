/dts-v1/;
#include "rk3308.dtsi"
#include <dt-bindings/pwm/pwm.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "CPUTOOLS I6S";
	compatible = "i2c,cputools-i6s", "rockchip,rk3308";

	aliases {
		ethernet0 = &gmac;
		mmc0 = &emmc;
		mmc1 = &sdmmc;
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:1500000n8";
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&green_led_gio>, <&heartbeat_led_gpio>;

		green-led {
			default-state = "on";
			gpios = <&gpio0 RK_PA6 GPIO_ACTIVE_HIGH>;
			label = "rockpis:green:power";
			linux,default-trigger = "timer";
		};

		blue-led {
			default-state = "on";
			gpios = <&gpio0 RK_PA5 GPIO_ACTIVE_HIGH>;
			label = "rockpis:blue:user";
			linux,default-trigger = "heartbeat";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		autorepeat;
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_buttons_pin>;

		button1 {
			label = "button1";
			linux,code = <KEY_POWER>;
			gpios = <&gpio2 RK_PA6 GPIO_ACTIVE_LOW>;
			// interrupt-parent = <&gpio2>;
			// interrupts = <RK_PA6 IRQ_TYPE_EDGE_BOTH>;
			debounce-interval = <100>;
			wakeup-source;
		};

		button2 {
			label = "button2";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio2 RK_PA7 GPIO_ACTIVE_LOW>;
			// interrupt-parent = <&gpio2>;
			// interrupts = <RK_PA7 IRQ_TYPE_EDGE_BOTH>;
			debounce-interval = <100>;
			wakeup-source;
		};

		button3 {
			label = "button3";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio2 RK_PB1 GPIO_ACTIVE_LOW>;
			// interrupt-parent = <&gpio2>;
			// interrupts = <RK_PB1 IRQ_TYPE_EDGE_BOTH>;
			debounce-interval = <100>;
			wakeup-source;
		};
	};

	backlight: backlight {
		compatible = "gpio-backlight";
		pinctrl-names = "default";
		pinctrl-0 = <&backlight_pins>;
		// pwms = <&pwm 0 1000000 PWM_POLARITY_INVERTED>;
		// brightness-levels = <0 30 40 50 60 70 100>;
		gpios = <&gpio0 RK_PC1 GPIO_ACTIVE_HIGH>; //  GPIO0_C1
		backlight-boot-on;
		status = "okay";
	};

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		pinctrl-0 = <&wifi_enable_h>;
		pinctrl-names = "default";
		reset-gpios = <&gpio0 RK_PA2 GPIO_ACTIVE_LOW>;
	};

	vcc_1v8: vcc-1v8 {
		compatible = "regulator-fixed";
		regulator-name = "vcc_1v8";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&vcc_io>;
	};

	vcc_io: vcc-io {
		compatible = "regulator-fixed";
		regulator-name = "vcc_io";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc_ddr: vcc-ddr {
		compatible = "regulator-fixed";
		regulator-name = "vcc_ddr";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1500000>;
		regulator-max-microvolt = <1500000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc5v0_otg: vcc5v0-otg {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&otg_vbus_drv>;
		regulator-name = "vcc5v0_otg";
		regulator-always-on;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	vdd_core: vdd-core {
		compatible = "pwm-regulator";
		pwms = <&pwm0 0 5000 1>;
		pwm-supply = <&vcc5v0_sys>;
		regulator-name = "vdd_core";
		regulator-min-microvolt = <827000>;
		regulator-max-microvolt = <1340000>;
		regulator-init-microvolt = <1015000>;
		regulator-settling-time-up-us = <250>;
		regulator-always-on;
		regulator-boot-on;
	};

	vdd_log: vdd-log {
		compatible = "regulator-fixed";
		regulator-name = "vdd_log";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1050000>;
		regulator-max-microvolt = <1050000>;
		vin-supply = <&vcc5v0_sys>;
	};
};

&cpu0 {
	cpu-supply = <&vdd_core>;
};

&emmc {
	bus-width = <4>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	non-removable;
	vmmc-supply = <&vcc_io>;
	status = "okay";
};

&gmac {
	clock_in_out = "output";
	phy-supply = <&vcc_io>;
	snps,reset-gpio = <&gpio0 RK_PA7 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	snps,reset-delays-us = <0 50000 50000>;
	local-mac-address = [a4 51 29 00 00 08];
	status = "okay";
};

&i2c1 {
	status = "okay";
	pinctrl-names = "default";
    pinctrl-0 = <&i2c1_xfer>;

	mpu6050@68 {
		compatible = "invensense,mpu6050";
		reg = <0x68>;
		status = "okay";
		interrupt-parent = <&gpio2>;
		interrupts = <RK_PB0 IRQ_TYPE_EDGE_RISING>; //GPIO2_B0
		mount-matrix = "-0.984807753012208",  /* x0 */
		               "0",                   /* y0 */
		               "-0.173648177666930",  /* z0 */
		               "0",                   /* x1 */
		               "-1",                  /* y1 */
		               "0",                   /* z1 */
		               "-0.173648177666930",  /* x2 */
		               "0",                   /* y2 */
		               "0.984807753012208";   /* z2 */
	};
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&rtc_32k>;

	leds {
		green_led_gio: green-led-gpio {
			rockchip,pins = <0 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		heartbeat_led_gpio: heartbeat-led-gpio {
			rockchip,pins = <0 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	buttons {
		gpio_buttons_pin: gpio-buttons {
			rockchip,pins =
				<2 RK_PA6 0 &pcfg_pull_up>,
				<2 RK_PA7 0 &pcfg_pull_up>,
				<2 RK_PB1 0 &pcfg_pull_up>;
		};
	};

	usb {
		otg_vbus_drv: otg-vbus-drv {
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <0 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		wifi_host_wake: wifi-host-wake {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	uart0 {
		uart0_xfer: uart0-xfer {
			rockchip,pins =
				<2 RK_PA1 1 &pcfg_pull_up>,
				<2 RK_PA0 1 &pcfg_pull_up>;
		};
	};

	uart1 {
		uart1_xfer: uart1-xfer {
			rockchip,pins =
				<1 RK_PD1 1 &pcfg_pull_up>,
				<1 RK_PD0 1 &pcfg_pull_up>;
		};
	};

	uart2-m0 {
		uart2m0_xfer: uart2m0-xfer {
			rockchip,pins =
				<1 RK_PC7 2 &pcfg_pull_up>,
				<1 RK_PC6 2 &pcfg_pull_up>;
		};
	};

	uart3 {
		uart3_xfer: uart3-xfer {
			rockchip,pins =
				<3 RK_PB5 4 &pcfg_pull_up>,
				<3 RK_PB4 4 &pcfg_pull_up>;
		};
	};

	uart4 {
		uart4_xfer: uart4-xfer {
			rockchip,pins =
				<4 RK_PB1 1 &pcfg_pull_up>,
				<4 RK_PB0 1 &pcfg_pull_up>;
		};

		uart4_cts: uart4-cts {
			rockchip,pins =
				<4 RK_PA6 1 &pcfg_pull_none>;
		};

		uart4_rts: uart4-rts {
			rockchip,pins =
				<4 RK_PA7 1 &pcfg_pull_none>;
		};
	};

	pwm2 {
		pwm2_pin: pwm2-pin {
			rockchip,pins =
				<0 RK_PB7 1 &pcfg_pull_none>;
		};
	};

	pwm3 {
		pwm3_pin: pwm3-pin {
			rockchip,pins =
				<0 RK_PC0 1 &pcfg_pull_none>;
		};
	};

	spi1 {
		spi1_csn0: spi1-csn0 {
			rockchip,pins =
				<3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		spi1_csn1: spi1-csn1 {
			rockchip,pins =
				<2 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		spi1_clk: spi1-clk {
			rockchip,pins =
				<3 RK_PB3 3 &pcfg_pull_up_4ma>;
		};

		spi1_miso: spi1-miso {
			rockchip,pins =
				<3 RK_PB2 3 &pcfg_pull_up_4ma>;
		};

		spi1_mosi: spi1-mosi {
			rockchip,pins =
				<3 RK_PB4 3 &pcfg_pull_up_4ma>;
		};
	};

	i2c1 {
		i2c1_xfer: i2c1-xfer {
			rockchip,pins =
				<0 RK_PB3 1 &pcfg_pull_none_smt>,
				<0 RK_PB4 1 &pcfg_pull_none_smt>;
		};
	};

	backlight_pins {
		backlight_pins: backlight-pins {
			rockchip,pins =
				<0 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	t_irq {
		t_irq: t_irq {
			rockchip,pins =
				<2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	tft_gpio_pins {
		tft_gpio_pins: tftg-pio-pins {
			rockchip,pins =
				<2 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>,
				<2 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	rc522 {
		rc522_rst: rc522-rst {
			rockchip,pins = <2 RK_PC0 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};

&pwm0 {
	status = "okay";
	pinctrl-0 = <&pwm0_pin_pull_down>;
};

&pwm2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pwm2_pin>;
};

&pwm3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pwm3_pin>;
};

&saradc {
	vref-supply = <&vcc_1v8>;
	status = "okay";
};

&sdio {
	#address-cells = <1>;
	#size-cells = <0>;
	cap-sd-highspeed;
	cap-sdio-irq;
	keep-power-in-suspend;
	max-frequency = <1000000>;
	mmc-pwrseq = <&sdio_pwrseq>;
	non-removable;
	sd-uhs-sdr104;
	status = "okay";
};

&sdmmc {
	cap-sd-highspeed;
	status = "okay";
};

&u2phy {
	status = "okay";

	u2phy_host: host-port {
		phy-supply = <&vcc5v0_otg>;
		status = "okay";
	};

	u2phy_otg: otg-port {
		phy-supply = <&vcc5v0_otg>;
		status = "okay";
	};
};

&uart0 {
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_xfer>;
	status = "okay";
};

&uart2 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart2m0_xfer>;
	status = "okay";
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart3_xfer>;
	status = "disabled";
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart4_xfer &uart4_cts &uart4_rts>;
	uart-has-rtscts;
	status = "okay";

	bluetooth {
		compatible = "realtek,rtl8723bs-bt";
		enable-gpios = <&gpio4 RK_PB2 GPIO_ACTIVE_HIGH>;
		device-wake-gpios = <&gpio4 RK_PB3 GPIO_ACTIVE_HIGH>;
		host-wake-gpios = <&gpio4 RK_PB4 GPIO_ACTIVE_HIGH>;
		max-speed = <1500000>;
	};
};

&spi1 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
    pinctrl-names = "default";
    pinctrl-0 = <&spi1_csn0 &spi1_csn1 &spi1_clk &spi1_miso &spi1_mosi>;
	spi-num-chipselects = <2>;
	cs-gpios = <&gpio3 RK_PB5 GPIO_ACTIVE_HIGH>, <&gpio2 RK_PB6 GPIO_ACTIVE_HIGH>;
    status = "okay";

/*
	ili9341@0 {
		compatible = "ilitek,ili9341";
		reg = <0>;
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&tft_gpio_pins>;
