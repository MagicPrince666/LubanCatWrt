	display@0{
		compatible = "adafruit,yx240qv29", "ilitek,ili9341";
		pinctrl-names = "default";
		pinctrl-0 = <&tft_gpio_pins>;
		reg = <0>;
		spi-max-frequency = <10000000>;
		dc-gpios = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio2 RK_PC0 GPIO_ACTIVE_LOW>;
		rotation = <270>;
		bgr;
		fps = <30>;
		buswidth = <8>;
		backlight = <&backlight>;
	};

	tsc2046@1 {
		reg = <1>;	/* CS1 */
		compatible = "ti,tsc2046";
		pinctrl-names = "default";
		pinctrl-0 = <&t_irq>;
		interrupt-parent = <&gpio2>;
		interrupts = <RK_PB5 IRQ_TYPE_EDGE_FALLING>;	// GPIO2_B5
		spi-max-frequency = <2000000>;
		pendown-gpio = <&gpio2 RK_PB5 GPIO_ACTIVE_HIGH>;
		vcc-supply = <&vcc_io>;

		ti,x-min = /bits/ 16 <0>;
		ti,x-max = /bits/ 16 <320>;
		ti,y-min = /bits/ 16 <0>;
		ti,y-max = /bits/ 16 <240>;
		ti,x-plate-ohms = /bits/ 16 <40>;
		ti,pressure-max = /bits/ 16 <255>;
		wakeup-source;
		status = "okay";
	};

	rc522@3{
		compatible = "nxp,rc522";
		spi-max-frequency = <8000000>;
		reg = <3>;
		rst-gpio = <&gpio2 RK_PC0 GPIO_ACTIVE_HIGH>;
 		pinctrl-names = "default";
		pinctrl-0 = <&rc522_rst>;
		//spi-cpha;
		//spi-cpol;
		status = "disabled";
	};
};

&spi2 {
	status = "disabled";
};

&usb_host_ehci {
	status = "okay";
};

&usb_host_ohci {
	status = "okay";
};

&usb20_otg {
	dr_mode = "peripheral";
	status = "okay";
};

&wdt {
	status = "okay";
};
