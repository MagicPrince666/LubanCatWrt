/dts-v1/;
#include "rk3308.dtsi"
#include <dt-bindings/pwm/pwm.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "CPUTOOLS I6P";
	compatible = "i2c,cputools-i6s", "rockchip,rk3308";

	aliases {
		ethernet0 = &gmac;
		mmc0 = &emmc;
		mmc1 = &sdmmc;
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:1500000n8";
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&green_led_gio>, <&heartbeat_led_gpio>;

		green-led {
			default-state = "on";
			gpios = <&gpio0 RK_PA6 GPIO_ACTIVE_HIGH>;
			label = "rockpis:green:power";
			linux,default-trigger = "timer";
		};
	};

	gpio_keys: gpio-keys {
		compatible = "gpio-keys";
		autorepeat;
	};

	backlight: backlight {
        compatible = "pwm-backlight";
		label = "backlight0";
		pwms = <&pwm1 0 50000 0>;
		brightness-levels = <
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			95  95  95  95  95  95  95  95
			88  89  90  91  92  93  94  95
			96  97  98  99 100 101 102 103
			104 105 106 107 108 109 110 111
			112 113 114 115 116 117 118 119
			120 121 122 123 124 125 126 127
			128 129 130 131 132 133 134 135
			136 137 138 139 140 141 142 143
			144 145 146 147 148 149 150 151
			152 153 154 155 156 157 158 159
			160 161 162 163 164 165 166 167
			168 169 170 171 172 173 174 175
			176 177 178 179 180 181 182 183
			184 185 186 187 188 189 190 191
			192 193 194 195 196 197 198 199
			200 201 202 203 204 205 206 207
			208 209 210 211 212 213 214 215
			216 217 218 219 220 221 222 223
			224 225 226 227 228 229 230 231
			232 233 234 235 236 237 238 239
			240 241 242 243 244 245 246 247
			248 249 250 251 252 253 254 255
		>;
		default-brightness-level = <128>;
        status = "okay";
    };

	sdio_pwrseq: sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		pinctrl-0 = <&wifi_enable_h>;
		pinctrl-names = "default";
		reset-gpios = <&gpio4 RK_PC0 GPIO_ACTIVE_LOW>;
	};

	tft_show: tft-show {
		compatible = "spi-gpio";
		#address-cells = <0x1>;
		#size-cells = <0x0>;
		pinctrl-0 = <&spi_gpio_pins &reset_pins>;
		pinctrl-names = "default";

		cs-gpios = <&gpio2 RK_PB5 GPIO_ACTIVE_LOW>;
		sck-gpios = <&gpio2 RK_PA6 GPIO_ACTIVE_LOW>;
		mosi-gpios = <&gpio2 RK_PB3 GPIO_ACTIVE_LOW>;
		num-chipselects = <1>;

		st7701s@0 {
			compatible = "sitronix,st7701s";
			reg = <0>;
			spi-max-frequency =<1000000>;
			rotate = <0>;
			spi-cpol;
			spi-cpha;
			rgb;
			fps = <10>;
			buswidth = <9>;
			reset-gpios = <&gpio0 RK_PC4 GPIO_ACTIVE_LOW>;
			dc-gpios = <&gpio1 RK_PA3 GPIO_ACTIVE_HIGH>;
			debug = <0>;
			backlight = <&backlight>;
			status = "okay";
		};
    };

	vcc_1v8: vcc-1v8 {
		compatible = "regulator-fixed";
		regulator-name = "vcc_1v8";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		vin-supply = <&vcc_io>;
	};

	vcc_io: vcc-io {
		compatible = "regulator-fixed";
		regulator-name = "vcc_io";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc_ddr: vcc-ddr {
		compatible = "regulator-fixed";
		regulator-name = "vcc_ddr";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1500000>;
		regulator-max-microvolt = <1500000>;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc5v0_otg: vcc5v0-otg {
		compatible = "regulator-fixed";
		enable-active-high;
		gpio = <&gpio0 RK_PC5 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&otg_vbus_drv>;
		regulator-name = "vcc5v0_otg";
		regulator-always-on;
		vin-supply = <&vcc5v0_sys>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	vdd_core: vdd-core {
		compatible = "pwm-regulator";
		pwms = <&pwm0 0 5000 1>;
		pwm-supply = <&vcc5v0_sys>;
		regulator-name = "vdd_core";
		regulator-min-microvolt = <827000>;
		regulator-max-microvolt = <1340000>;
		regulator-init-microvolt = <1015000>;
		regulator-settling-time-up-us = <250>;
		regulator-always-on;
		regulator-boot-on;
	};

	vdd_log: vdd-log {
		compatible = "regulator-fixed";
		regulator-name = "vdd_log";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <1050000>;
		regulator-max-microvolt = <1050000>;
		vin-supply = <&vcc5v0_sys>;
	};

	display_subsystem: display-subsystem {
		compatible = "rockchip,display-subsystem";
		ports = <&vop_out>;
		logo-memory-region = <&drm_logo>;
		status = "disabled";

		devfreq = <&dmc>;
		route {
			route_rgb: route-rgb {
				status = "disabled";
				logo,uboot = "logo.bmp";
				/* logo,kernel = "logo_kernel.bmp"; */
				logo,mode = "center";
				charge_logo,mode = "center";
				connect = <&vop_out_rgb>;
			};
		};
	};

	panel: panel {
		compatible = "auo,b101ean01";
		status = "disabled";
		/* enable-gpios = <&gpio7 21 GPIO_ACTIVE_HIGH>; */
		bus-format = <0x100a>;

		ports {
			panel_in_rgb: endpoint {
				remote-endpoint = <&rgb_out_panel>;
			};
		};
	};

	rgb: rgb {
		compatible = "rockchip,rk3288-lvds";
		status = "disabled";
		pinctrl-names = "default";
		pinctrl-0 = <&lcdc_ctl>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				#address-cells = <1>;
				#size-cells = <0>;

				rgb_in_vop: endpoint@0 {
					reg = <0>;
					remote-endpoint = <&vop_out_rgb>;
				};
			};

			rgb_out: port@1 {
				reg = <1>;
				#address-cells = <1>;
				#size-cells = <0>;

				rgb_out_panel: endpoint {
					remote-endpoint = <&panel_in_rgb>;
				};
			};

		};
	};

	vop: vop@ff2e0000 {
		compatible = "rockchip,rk3308-vop";
		reg = <0x0 0xff2e0000 0x0 0x1fc>, <0x0 0xff2e0a00 0x0 0x400>;
		reg-names = "regs", "gamma_lut";
		interrupts = <GIC_SPI 46 IRQ_TYPE_LEVEL_HIGH>;
		clocks = <&cru ACLK_VOP>, <&cru DCLK_VOP>,
			 <&cru HCLK_VOP>;
		clock-names = "aclk_vop", "dclk_vop", "hclk_vop";
		rockchip,grf = <&grf>;
		status = "disabled";

		vop_out: port {
			#address-cells = <1>;
			#size-cells = <0>;

			vop_out_rgb: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&rgb_in_vop>;
			};
		};
	};
};

&cpu0 {
	cpu-supply = <&vdd_core>;
};

&emmc {
	pinctrl-names = "default";
    pinctrl-0 = <&emmc_clk &emmc_cmd &emmc_bus8>;
	bus-width = <8>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	non-removable;
	vmmc-supply = <&vcc_io>;
	status = "okay";
};

&display_subsystem {
	status = "okay";
};

&panel {
	compatible = "simple-panel";
	/* compatible = "auo,b101ean01"; *//* 1280 x 800 */
	/* compatible = "avic,tm070ddh03"; *//* 1024 x 600 */
	/* compatible = "innolux,zj070na-01p"; *//* 1024 x 600 */
	/* MEDIA_BUS_FMT_RGB888_1X24 MEDIA_BUS_FMT_RGB666_1X18 MEDIA_BUS_FMT_RGB565_1X16 MEDIA_BUS_FMT_RGB666_1X24_CPADHI */
	bus-format = <MEDIA_BUS_FMT_RGB666_1X18>;
	backlight = <&backlight>;
	status = "okay";

	reset-delay-ms = <120>;
	init-delay-ms = <120>;
	enable-delay-ms = <120>;
	prepare-delay-ms = <120>;
	disable-delay-ms = <120>;
	unprepare-delay-ms = <120>;

	display-timings {
		native-mode = <&mdk3308_lcd1024_timing>;

		mdk3308_lcd1024_timing: timing0 {
			clock-frequency = <36600000>;
			hactive = <1024>;
			vactive = <600>;
			hback-porch = <140>;
			hfront-porch = <160>;
			vback-porch = <20>;
			vfront-porch = <12>;
			hsync-len = <20>;
			vsync-len = <3>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <1>;
		};

		mdk3308_lcd800_timing: timing1 {
			clock-frequency = <33000000>;
			hactive = <800>;
			vactive = <480>;
			hback-porch = <40>;
			hfront-porch = <40>;
			vback-porch = <29>;
			vfront-porch = <13>;
			hsync-len = <48>;
			vsync-len = <3>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <1>;
		};

		mdk3308_lcd480_timing: timing2 {
			clock-frequency = <9000000>;
			hactive = <480>;
			vactive = <272>;
			hback-porch = <2>;
			hfront-porch = <2>;
			vback-porch = <2>;
			vfront-porch = <2>;
			hsync-len = <41>;
			vsync-len = <10>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <1>;
		};

		mdk3308_lcd320480_timing: timing3 {
			clock-frequency = <11000000>;
			hactive = <320>;
			vactive = <480>;
			hback-porch = <10>;
			hfront-porch = <4>;
			vback-porch = <10>;
			vfront-porch = <4>;
			hsync-len = <20>;
			vsync-len = <20>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
			pixelclk-active = <0>;
		};
	};
};

&rgb {
	/delete-property/ pinctrl-0;
	status = "okay";
};

&vop {
	status = "okay";
};

&gmac {
	status = "disabled";
};

&i2c1 {
	compatible = "rockchip,rk3399-i2c";
	status = "okay";
	pinctrl-names = "default";
    pinctrl-0 = <&i2c1_xfer>;

	touchscreen@5d {
		compatible = "goodix,gt911";
		reg = <0x5d>;
		pinctrl-names = "default";
		pinctrl-0 = <&tp_int &tp_reset>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PB0 IRQ_TYPE_EDGE_FALLING>;
		irq-gpios = <&gpio0 RK_PB0 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio0 RK_PC0 GPIO_ACTIVE_LOW>;
		// touchscreen-swapped-x-y;
		touchscreen-size-x = <480>;
		touchscreen-size-y = <800>;
		status = "okay";
	};
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&rtc_32k>;

	sdmmc {
		sdmmc_clk: sdmmc-clk {
			rockchip,pins =
				<4 RK_PD5 1 &pcfg_pull_none_4ma>;
		};

		sdmmc_cmd: sdmmc-cmd {
			rockchip,pins =
				<4 RK_PD4 1 &pcfg_pull_up_4ma>;
		};

		sdmmc_det: sdmmc-det {
			rockchip,pins =
				<0 RK_PA3 1 &pcfg_pull_up_4ma>;
		};

		sdmmc_pwren: sdmmc-pwren {
			rockchip,pins =
				<4 RK_PD6 1 &pcfg_pull_none_4ma>;
		};

		sdmmc_bus1: sdmmc-bus1 {
			rockchip,pins =
				<4 RK_PD0 1 &pcfg_pull_up_4ma>;
		};

		sdmmc_bus4: sdmmc-bus4 {
			rockchip,pins =
				<4 RK_PD0 1 &pcfg_pull_up_4ma>,
				<4 RK_PD1 1 &pcfg_pull_up_4ma>,
				<4 RK_PD2 1 &pcfg_pull_up_4ma>,
				<4 RK_PD3 1 &pcfg_pull_up_4ma>;
		};

		sdmmc_bus8: sdmmc-bus8 {
			rockchip,pins =
				<4 RK_PD0 0 &pcfg_pull_up_4ma>,
				<4 RK_PD1 0 &pcfg_pull_up_4ma>,
				<4 RK_PD2 0 &pcfg_pull_up_4ma>,
				<4 RK_PD3 0 &pcfg_pull_up_4ma>,
				<4 RK_PD4 0 &pcfg_pull_up_4ma>,
				<4 RK_PD5 0 &pcfg_pull_up_4ma>,
				<4 RK_PD6 0 &pcfg_pull_up_4ma>;
		};
	};

	leds {
		green_led_gio: green-led-gpio {
			rockchip,pins = <0 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		heartbeat_led_gpio: heartbeat-led-gpio {
			rockchip,pins = <0 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb {
		otg_vbus_drv: otg-vbus-drv {
			rockchip,pins = <0 RK_PC5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = 
				<4 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	uart0 {
		uart0_xfer: uart0-xfer {
			rockchip,pins =
				<2 RK_PA1 1 &pcfg_pull_up>,
				<2 RK_PA0 1 &pcfg_pull_up>;
		};
	};

	uart4 {
		uart4_xfer: uart4-xfer {
			rockchip,pins =
				<4 RK_PB1 1 &pcfg_pull_up>,
				<4 RK_PB0 1 &pcfg_pull_up>;
		};

		uart4_cts: uart4-cts {
			rockchip,pins =
				<4 RK_PA6 1 &pcfg_pull_none>;
		};

		uart4_rts: uart4-rts {
			rockchip,pins =
				<4 RK_PA7 1 &pcfg_pull_none>;
		};
	};

	bluetooth {
		bluetooth_pins: bluetooth-rts {
			rockchip,pins =
				<4 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>,
				<4 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>,
				<4 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};

	pwm1 {
		pwm1_pin: pwm1-pin {
			rockchip,pins =
				<0 RK_PB6 1 &pcfg_pull_none>;
		};
	};

	lcd_pins {
		lcd_pins: lcd-pins {
			rockchip,pins =
				<2 RK_PC0 1 &pcfg_pull_up>, // R7
				<2 RK_PB7 1 &pcfg_pull_up>, // R6
				<2 RK_PB2 1 &pcfg_pull_up>, // R5
				<2 RK_PB1 1 &pcfg_pull_up>, // R4
				<1 RK_PB6 1 &pcfg_pull_up>, // LCDC_D10 B2
				<1 RK_PB5 1 &pcfg_pull_up>, // LCDC_D9 B1
				<1 RK_PB4 1 &pcfg_pull_up>, // LCDC_D8 B0
				<1 RK_PB3 1 &pcfg_pull_up>, // LCDC_D7 G7
				<1 RK_PB2 1 &pcfg_pull_up>, // LCDC_D6 G6
				<1 RK_PB1 1 &pcfg_pull_up>, // LCDC_D5 G5
				<1 RK_PB0 1 &pcfg_pull_up>, // LCDC_D4 G4
				<1 RK_PA7 1 &pcfg_pull_up>, // LCDC_D3 G3
				<1 RK_PA6 1 &pcfg_pull_up>, // LCDC_D2 G2
				<1 RK_PA5 1 &pcfg_pull_up>, // LCDC_D1 G1
				<1 RK_PA4 1 &pcfg_pull_up>, // LCDC_D0 G0
				<1 RK_PC7 1 &pcfg_pull_up>, // spi2_mosi/uart2_tx R3
				<1 RK_PC6 1 &pcfg_pull_up>, // spi2_miso/uart2_rx R2
				<1 RK_PC5 1 &pcfg_pull_up>, // LCDC_D17 R1
				<1 RK_PC4 1 &pcfg_pull_up>, // LCDC_D16 R0
				<1 RK_PA3 1 &pcfg_pull_up>, // LCDC_DEN de
				<1 RK_PA2 1 &pcfg_pull_up>, // LCDC_VSYNC vs
				<1 RK_PA1 1 &pcfg_pull_up>, // LCDC_HSYC hs
				<1 RK_PA0 1 &pcfg_pull_up>, // LCDC_DCLK dclk
				<1 RK_PC3 1 &pcfg_pull_up>, // LCDC_D15 B7
				<1 RK_PC2 1 &pcfg_pull_up>, // LCDC_D14 B6
				<1 RK_PC1 1 &pcfg_pull_up>, // LCDC_D13 B5
				<1 RK_PC0 1 &pcfg_pull_up>, // LCDC_D12 B4
				<1 RK_PB7 1 &pcfg_pull_up>; // LCDC_D11 B3
		};

		lcdc_ctl: lcdc-ctl {
				rockchip,pins =
					/* dclk */
					<1 RK_PA0 RK_FUNC_1 &pcfg_pull_none>,
					/* hsync */
					<1 RK_PA1 RK_FUNC_1 &pcfg_pull_none>,
					/* vsync */
					<1 RK_PA2 RK_FUNC_1 &pcfg_pull_none>,
					/* den */
					<1 RK_PA3 RK_FUNC_1 &pcfg_pull_none>,
					/* d0 */
					<1 RK_PA4 RK_FUNC_1 &pcfg_pull_none>,
					/* d1 */
					<1 RK_PA5 RK_FUNC_1 &pcfg_pull_none>,
					/* d2 */
					<1 RK_PA6 RK_FUNC_1 &pcfg_pull_none>,
					/* d3 */
					<1 RK_PA7 RK_FUNC_1 &pcfg_pull_none>,
					/* d4 */
					<1 RK_PB0 RK_FUNC_1 &pcfg_pull_none>,
					/* d5 */
					<1 RK_PB1 RK_FUNC_1 &pcfg_pull_none>,
					/* d6 */
					<1 RK_PB2 RK_FUNC_1 &pcfg_pull_none>,
					/* d7 */
					<1 RK_PB3 RK_FUNC_1 &pcfg_pull_none>,
					/* d8 */
					<1 RK_PB4 RK_FUNC_1 &pcfg_pull_none>,
					/* d9 */
					<1 RK_PB5 RK_FUNC_1 &pcfg_pull_none>,
					/* d10 */
					<1 RK_PB6 RK_FUNC_1 &pcfg_pull_none>,
					/* d11 */
					<1 RK_PB7 RK_FUNC_1 &pcfg_pull_none>,
					/* d12 */
					<1 RK_PC0 RK_FUNC_1 &pcfg_pull_none>,
					/* d13 */
					<1 RK_PC1 RK_FUNC_1 &pcfg_pull_none>,
					/* d14 */
					<1 RK_PC2 RK_FUNC_1 &pcfg_pull_none>,
					/* d15 */
					<1 RK_PC3 RK_FUNC_1 &pcfg_pull_none>,
					/* d16 */
					<1 RK_PC4 RK_FUNC_1 &pcfg_pull_none>,
					/* d17 */
					<1 RK_PC5 RK_FUNC_1 &pcfg_pull_none>;
			};

			lcdc_rgb888_m0: lcdc-rgb888-m0 {
				rockchip,pins =
					/* d18 */
					<1 RK_PC6 RK_FUNC_6 &pcfg_pull_none>,
					/* d19 */
					<1 RK_PC7 RK_FUNC_6 &pcfg_pull_none>,
					/* d20 */
					<2 RK_PB1 RK_FUNC_3 &pcfg_pull_none>,
					/* d21 */
					<2 RK_PB2 RK_FUNC_3 &pcfg_pull_none>,
					/* d22 */
					<2 RK_PB7 RK_FUNC_3 &pcfg_pull_none>,
					/* d23 */
					<2 RK_PC0 RK_FUNC_3 &pcfg_pull_none>;
			};

			lcdc_rgb888_m1: lcdc-rgb888-m1 {
				rockchip,pins =
					/* d18 */
					<3 RK_PA6 RK_FUNC_3 &pcfg_pull_none>,
					/* d19 */
					<3 RK_PA7 RK_FUNC_3 &pcfg_pull_none>,
					/* d20 */
					<3 RK_PB0 RK_FUNC_3 &pcfg_pull_none>,
					/* d21 */
					<3 RK_PB1 RK_FUNC_3 &pcfg_pull_none>,
					/* d22 */
					<3 RK_PB2 RK_FUNC_4 &pcfg_pull_none>,
					/* d23 */
					<3 RK_PB3 RK_FUNC_4 &pcfg_pull_none>;
			};
	};

	rgb_tft {
		spi_gpio_pins: spi_gpio-pins {
			rockchip,pins =
				<2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>, // cs
				<2 RK_PA6 RK_FUNC_GPIO &pcfg_pull_up>, // scl
				<2 RK_PB3 RK_FUNC_GPIO &pcfg_pull_up>; // sdi
		};

		reset_pins: reset-pins {
			rockchip,pins =
				<0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_up>; // reset
		};
	};

	gt911 {
		i2c1_pins: i2c1-pins {
			rockchip,pins =
				<0 RK_PB3 1 &pcfg_pull_none_smt>,
				<0 RK_PB4 1 &pcfg_pull_none_smt>;
		};

		tp_int: tp-int {
			rockchip,pins =
				<0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		tp_reset: tp-reset {
			rockchip,pins =
				<0 RK_PC0 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};

